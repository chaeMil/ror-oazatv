<div class="row toolbar">
  <div class="col">
    <h3><%= t('admin.menu.video_convert_profiles') %> > <%= @profile.title %> </h3>
  </div>
  <div class="col text-right">
    <buton class="btn btn-secondary"
           onclick="javascript:history.back();">
      <%= t('admin.action.back') %>
    </buton>
    <%= link_to t('admin.label.show'),
                admin_video_convert_profile_path(@profile),
                class: 'btn btn-primary' %>
    <a href="#" data-submit-form="video_convert_profile_form"
       class="btn btn-success">
      <%= t('admin.action.save') %>
    </a>
  </div>
</div>

<%= render 'form', profile: @profile %>

